{
  "name": "kameleoon-test-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "optimize-svg": "svgo -r ./src/assets/icons",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:ts": "tsc -p tsconfig.json",
    "lint:js": "eslint src",
    "postinstall": "husky",
    "prepare": "husky",
    "upgrade-dependencies": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.svg": "svgo",
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings 0 --color",
    "*.{js,jsx,ts,tsx,css,scss,json,md}": "prettier --write"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "sass": "^1.94.1",
    "svgo": "^4.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-svgr": "^4.5.0"
  }
}
